{
  "name": "@hspotted/reactjs",
  "author": {
    "name": "Henrique Fernandes",
    "url": "https://github.com/hspotted"
  },
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
